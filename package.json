{
  "name": "@innofake/inno-random-joke",
  "version": "1.0.0",
  "description": "Web Component that displays a random joke.",
  "main": "inno-joker.js",
  "module": "inno-joker.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf *{d.ts,d.ts.map,js,js.map}",
    "publish": "npm publish --access public",
    "serve": "wds --watch",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innofake/inno-joker.git"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Stefan Buys",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/innofake/inno-joker/issues"
  },
  "homepage": "https://github.com/innofake/inno-joker#readme",
  "dependencies": {
    "@open-wc/testing": "^3.1.6",
    "lit": "^2.2.7"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.32",
    "@web/dev-server-legacy": "^1.0.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "@web/test-runner": "^0.13.31",
    "@web/test-runner-playwright": "^0.8.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
